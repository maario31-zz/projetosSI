// Código com repetição 
//Faça um programa em C que exiba todos os números pares e a soma dos números ímpares compreendidos entre 10 e 200 (exclusive).
#include <stdio.h>
#include <stdlib.h>

int main (){
	int i, soma = 0;
	
	for(i=10;i<=200;i++){
		if(i%2==0){
		printf("%d\n", i);
		}else{
			soma+=1;
		}
	}
		printf("Soma dos impares: %d\n", soma);
	return 0;
	system("pause>>NULL");
	
}
