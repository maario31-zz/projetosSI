#include <stdio.h>
#include <stdlib.h>

int main(){
int n,k,i,j;

printf("Digite quantos blocos você deseja na horizontal:\n");
scanf("%d", &n);
printf("Digite quantos blocos você deseja na vertical:\n");
scanf("%d", &k);
for(i=0;i<n;i++){
printf("[ ]");
for(j=0;j<k;j++){
printf("[ ]");
}
printf("\n");
}

return 0;
}
